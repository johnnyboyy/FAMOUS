<%= form_for(@song) do |f| %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :artist %><br>
    <%= f.text_field :artist %>
  </div>
  <div class="field">
    <%=f.label :mp3_file %>
    <%=f.file_field :mp3_file %>
  </div>

  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
